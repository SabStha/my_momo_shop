                        <div class="amako-step amako-step-welcome">
                            <div class="mascot-container">
                                <img src="/storage/logo/ama.gif" alt="Ama" class="mascot-avatar" onerror="this.style.display='none'; this.nextElementSibling.style.display='block';">
